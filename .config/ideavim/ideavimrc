source ~/.vimrc
""" Plugins  --------------------------------
set surround
set multiple-cursors
set commentary
set argtextobj
set easymotion
set textobj-entire
set ReplaceWithRegister

""" Plugin settings -------------------------
let g:argtextobj_pairs="[:],(:),<:>"

map <leader> <Plug>(easymotion-prefix)
imap jj <Esc>
set timeoutlen=1000
