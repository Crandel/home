#!/usr/bin/env bash

declare -a arr=(
  "Books"
  "DCIM"
  "Diaro"
  "Documents"
  "Download"
  "Fonts"
  "MapsWithMe"
  "Music"
  "Pictures"
  "SamLib-Info"
  "TWRP"
  "TitaniumBackup"
  "VocalizerEx2"
  "dictdata"
  "ivona"
  "progs"
  "termux"
  "viber"
  "work"
)



for FolderStr in "${arr[@]}"
do
    echo "$FolderStr"
    adb-sync --reverse /sdcard/$FolderStr/ /data/backup/android/xiaomi_mi_a1/$FolderStr/
    sleep 2
done
