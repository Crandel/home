#! /bin/bash

while ! ping -c 1 8.8.8.8 > /dev/null ; do
  sleep 0.5
done


sleep 0.5
echo "Start thunderbird" >> /tmp/network-startup.log
thunderbird &



sleep 0.5
echo "Start telegram" >> /tmp/network-startup.log
telegram-desktop &



sleep 0.5
echo "Start RSS reader" >> /tmp/network-startup.log
rssguard &

