{
  "aria2": {
    "args": [
      "--allow-overwrite=true",
      "--always-resume=false",
      "--auto-file-renaming=false",
      "--check-integrity=true",
      "--conditional-get=true",
      "--continue=true",
      "--split=10",
      "--file-allocation=none",
      "--console-log-level=error",
      "--max-concurrent-downloads=100",
      "--max-connection-per-server=10",
      "--min-split-size=5M",
      "--remote-time=true",
      "--show-console-readout=true"
    ],
    "path": "/usr/bin/aria2c"
  },
  "pacman": {
    "config": "/etc/pacman.conf",
    "path": "/usr/bin/pacman"
  },
  "pacserve": {
    "server": null
  },
  "powerpill": {
    "select": true,
    "reflect databases": false
  },
  "reflector": {
    "args.unused": [
      "--protocol",
      "http",
      "--latest",
      "10",
      "--sort",
      "rate"
    ]
  },
  "rsync": {
    "args": [
      "-q",
      "--compress",
      "--no-motd",
      "--progress"
    ],
    "db only": true,
    "path": "/usr/bin/rsync",
    "servers": [
      "rsync://ftp.hosteurope.de/archlinux/$repo/os/$arch",
      "rsync://mirrors.rit.edu/archlinux/$repo/os/$arch",
      "rsync://ftp.nluug.nl/archlinux/$repo/os/$arch",
      "rsync://ftp.gwdg.de/pub/linux/archlinux/$repo/os/$arch",
      "rsync://mirror.puzzle.ch/archlinux/$repo/os/$arch",
      "rsync://rsync.gtlib.gatech.edu/archlinux/$repo/os/$arch",
      "rsync://mirrors.uk2.net/archlinux/$repo/os/$arch",
      "rsync://mirror.csclub.uwaterloo.ca/archlinux/$repo/os/$arch",
      "rsync://mirror.umd.edu/archlinux/$repo/os/$arch",
      "rsync://ftp.iinet.net.au/archlinux/$repo/os/$arch"
    ]
  }
}
