{
  "aria2": {
    "args": [
      "--allow-overwrite=true",
      "--always-resume=false",
      "--auto-file-renaming=false",
      "--check-integrity=true",
      "--conditional-get=true",
      "--continue=true",
      "--file-allocation=none",
      "--console-log-level=error",
      "--max-concurrent-downloads=100",
      "--max-connection-per-server=5",
      "--min-split-size=5M",
      "--remote-time=true",
      "--show-console-readout=true"
    ],
    "path": "/usr/bin/aria2c"
  },
  "pacman": {
    "config": "/etc/pacman.conf",
    "path": "/usr/bin/pacman"
  },
  "pacserve": {
    "server": null
  },
  "powerpill": {
    "select": true,
    "reflect databases": false
  },
  "reflector": {
    "args.unused": [
      "--protocol",
      "http",
      "--latest",
      "10",
      "--sort",
      "rate"
    ]
  },
  "rsync": {
    "args": [
      "--no-motd",
      "--progress"
    ],
    "db only": true,
    "path": "/usr/bin/rsync",
    "servers": [
        "rsync://mirror.f4st.host/archlinux/$repo/os/$arch",
        "rsync://ftp.sh.cvut.cz/arch/$repo/os/$arch",
        "rsync://ftp.halifax.rwth-aachen.de/archlinux/$repo/os/$arch",
        "rsync://rsync.osbeck.com/archlinux/$repo/os/$arch",
        "rsync://mirrors.atviras.lt/archlinux/$repo/os/$arch",
        "rsync://arch.mirror.square-r00t.net/arch/$repo/os/$arch",
        "rsync://mex.mirror.pkgbuild.com/packages/$repo/os/$arch",
        "rsync://ind.mirror.pkgbuild.com/packages/$repo/os/$arch",
        "rsync://jpn.mirror.pkgbuild.com/packages/$repo/os/$arch",
        "rsync://mirrors.neusoft.edu.cn/archlinux/$repo/os/$arch"
    ]
  }
}
